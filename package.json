  "scripts": {
    "build:dev": "webpack --config config/webpack.dev.js --mode development",
    "build:prod": "webpack --config config/webpack.prod.js -p --mode production",
    "start": "npm run start:prod",
    "start:dev": "webpack-dev-server --config config/webpack.dev.js --mode development --progress --inline --watch --host 0.0.0.0 --port 8088",
    "start:prod": "node scripts/prod-server.js",
    "test": "npm run lint && karma start",
    "lint": "npm run tslint \"src/**/*.ts\"",
    "tslint": "tslint",
    "postinstall": "npm run build:prod",
    "prestart": "echo 'Starting the dev server.'",
    "prestart:prod": "echo 'Starting the prod server.'",
    "reinstall": "rm -rf node_modules && npm install"
  },
